{
  "name": "checklist-backedn",
  "version": "0.0.1",
  "description": "Checklist Backend",
  "author": "DevDBreno <dbrenoti@gmail.com>",
  "license": "MIT",
  "scripts": {
    "db:init": "yarn db:create && yarn db:migrate && yarn db:seed",
    "db:reset": "yarn db:drop && yarn db:init",
    "db:create": "yarn sequelize db:create",
    "db:migrate": "yarn sequelize db:migrate",
    "db:seed": "yarn sequelize db:seed:all",
    "db:drop": "yarn sequelize db:drop",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:docker": "docker-compose up",
    "start:prod": "yarn build && node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.11",
    "@nestjs/core": "^6.10.11",
    "@nestjs/platform-express": "^6.10.11",
    "@nestjs/platform-socket.io": "^6.10.14",
    "@nestjs/websockets": "^6.10.14",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.11.0",
    "dotenv": "^8.2.0",
    "mysql2": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^1.1.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.12.9",
    "@nestjs/schematics": "^6.7.6",
    "@nestjs/testing": "^6.10.11",
    "@types/bluebird": "^3.5.29",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.21",
    "@types/sequelize": "^4.28.8",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^12.0.1",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
